# Server Configuration
PORT=5000
NODE_ENV=development

# Database Configuration
DB_HOST=127.0.0.1
DB_USER=root
DB_PASSWORD=your_database_password
DB_NAME=iware_warehouse

# JWT Secret (Generate dengan: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))")
JWT_SECRET=your_jwt_secret_key_here
JWT_EXPIRE=7d

# Accurate Online API Token
# Dapatkan dari: https://account.accurate.id/developer/api-token
ACCURATE_API_URL=https://public-api.accurate.id/api
ACCURATE_APP_KEY=your_app_key_here
ACCURATE_SIGNATURE_SECRET=your_signature_secret_here
ACCURATE_ACCESS_TOKEN=your_api_token_here
ACCURATE_DATABASE_ID=your_database_id

# Accurate OAuth Configuration (Optional - jika menggunakan OAuth)
ACCURATE_CLIENT_ID=your_client_id_here
ACCURATE_CLIENT_SECRET=your_client_secret_here
ACCURATE_REDIRECT_URI=http://localhost:5000/api/accurate/callback

# CORS
CORS_ORIGIN=http://localhost:3000
